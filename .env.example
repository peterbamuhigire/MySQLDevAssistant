# Environment Variables Example
# Copy this file to .env and fill in your values

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your_password_here
DB_NAME=your_database_name

# Application Settings
APP_DEBUG=False
APP_LOG_LEVEL=INFO

# Backup Settings
BACKUP_ENABLED=True
BACKUP_LOCATION=./backups
